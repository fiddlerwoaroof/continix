{
  "name": "continix",
  "version": "1.0.0",
  "description": "A simple tree-based state container that provides an alternative state model to redux",
  "main": "dist/state_container.js",
  "scripts": {
    "test": "jest",
    "babel": "babel src/ -d dist/"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.16.0",
    "jest": "^21.2.1",
    "regenerator-runtime": "^0.13.2",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "immutable": "^3.8.2",
    "ramda": "^0.25.0",
    "sinon": "^7.3.2"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "babel": {
    "presets": [
      "env"
    ],
    "env": {
      "test": {
        "presets": [
          "env"
        ]
      }
    }
  }
}
